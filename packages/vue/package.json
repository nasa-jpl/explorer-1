    "tailwindcss": "^3.4.3",
    "tailwindcss-themer": "^4.0.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@explorer-1/common": "workspace:*",
    "@explorer-1/prettier-config": "workspace:*",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "typescript": "^5.2.2",
    "vite": "^5.2.11",
    "vue-tsc": "^2.0.6"
  }
